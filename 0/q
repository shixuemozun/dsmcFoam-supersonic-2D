FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      q;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
   IN
    {
         type            zeroGradient;
    }

 
WALL
    {
        type            calculated;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}

