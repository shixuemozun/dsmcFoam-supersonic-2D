FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      boundaryT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 273;

boundaryField
{
    IN
    {
        type            fixedValue;
        value           uniform 500;
    }

  
    WALL
    {
        type            fixedValue;
        value           uniform 273;
    }
  
    frontAndBack
    {
        type            empty;
    }
}

